cat > .gitignore <<'EOF'
# Node dependencies
node_modules/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Build output
dist/
build/

# IDE / Editor
.vscode/
.idea/
.DS_Store

# OS files
Thumbs.db

# Coverage reports
coverage/

# Temporary files
tmp/
temp/
EOF
